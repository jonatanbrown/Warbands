<h2><%= @team.name %></h2>
<div><%= link_to "Edit team details", edit_team_path %></div>
</br>
<div id="team-char-list" class="left">
  <h3>Warband members</h3>
  <div class="characters-list">
    <%= render :partial => "characters/character", :collection => @team.characters %>
  </div>
</div>
<div id="team-equipment-list" class="left">
  <h3>Unused Gear</h3>
  <div>
    <%= render :partial => "equipments/equipment", :collection => @team.unused_equipment %>
  </div>
</div>

